{
  "name": "GDoc Pivotal",
  "version": "1.5",
  "description": "GDoc Pivotal update statuses pivotal links in google documents",
  "background_page": "background.html",
  "options_page": "options.html",
  "browser_action": {
    "default_title": "GDoc Pivotal",
    "default_icon": "images/icon.png",
    "popup": "popup.html"
  },
  "icons": { 
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png" 
  },
  "permissions": [
    "tabs",
    "notifications",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ]
}
